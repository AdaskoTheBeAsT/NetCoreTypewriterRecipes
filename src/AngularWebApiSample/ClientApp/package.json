    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "stats": "ng build --stats-json",
    "stats-prod": "ng build --configuration production --stats-json",
    "stats-report": "webpack-bundle-analyzer ../wwwroot/stats.json",
    "type-check": "tsc --noEmit",
    "stylelint": "stylelint \"{apps,libs}/**/*.scss\" --syntax scss",
    "stylelint-check": "stylelint-config-prettier-check",
    "stylelint-report-json": "stylelint \"{apps,libs}/**/*.scss\" --syntax scss -f json>../../../reports/angular/stylelint-report.json",
    "stylelint-report-html": "stylelint \"{apps,libs}/**/*.scss\" --custom-formatter=node_modules/stylelint-html-formatter --syntax scss>../../../reports/angular/stylelint-report.html",
    "htmlhint": "htmlhint --config .htmlhintrc \"{apps,libs}/**/*.html\"",
    "htmlhint-report-html": "htmlhint --config .htmlhintrc \"{apps,libs}/**/*.html\" --format html&& mv report.html ../../../reports/angular/htmlhint-report.html || move report.html ../../../reports/angular/htmlhint-report.html",
    "htmlhint-report-txt": "htmlhint --config .htmlhintrc \"{apps,libs}/**/*.html\">../../../reports/angular/htmlhint-report.txt",
    "create-icon-sprite": "svg2sprite ./src/assets/svg ./src/assets/sprites/img.svg --stripAttrs id --stripTags title",
    "prepare": "cd .. && cd .. && cd .. && husky install src/AngularWebApiSample/ClientApp/.husky"
  },
