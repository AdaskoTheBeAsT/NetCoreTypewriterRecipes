{
  "name": "client-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ngcc": "ngcc",
    "start": "ng serve",
    "build": "ng build",
    "prod": "ng build --prod",
    "test": "jest --watch",
    "test2": "ng test",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "testcafe-live chrome e2e/**/*.e2e-spec.ts",
    "e2e:ci": "testcafe chrome e2e/**/*.e2e-spec.ts --app \"yarn start\" --app-init-delay 40000 --proxy-bypass http://localhost:4200/",
    "format": "prettier {src,e2e}/**/*.{ts,md,scss} --write",
    "style": "yarn format && yarn lint",
    "verify": "yarn format -l && yarn lint && jest --bail",
    "precommit": "lint-staged",
    "ts-lint": "tslint --project tsconfig.json -c tslint.json -o ../../../Cake/.artifacts/tslint-result.json -t json --force src/**/*.ts",
    "ts-lint-console": "tslint --project tsconfig.json -c tslint.json --force src/**/*.ts",
    "1postinstall": "ngcc",
    "stats": "ng build --stats-json",
    "stats-prod": "ng build --prod --stats-json",
    "stats-report": "webpack-bundle-analyzer ../wwwroot/stats.json",
    "type-check": "tsc --noEmit",
    "ts-lint-simple": "tslint --project tsconfig.json --config tslint.json  -o ../../../reports/epcalculator-tslint-result.json -t json --force src/**/*.ts",
    "stylelint-simple": "stylelint src/**/*.scss --syntax scss -f json>../../../reports/stylelint-report.json",
    "htmlhint-simple": "htmlhint src/**/*.html --config .htmlhintrc>../../../reports/htmlhint-report.txt",
    "create-icon-sprite": "svg2sprite ./src/assets/svg ./src/assets/sprites/img.svg --stripAttrs id --stripTags title"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx},!src/app/api/**": [
      "yarn lint",
      "git add"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md},!src/app/api/**": [
      "prettier --write",
      "git add"
    ]
  },
  "jestNunitReporter": {
    "outputPath": "../../../reports/frontendunittest/",
    "outputFilename": "frontend-test.nunit.xml"
  },
  "dependencies": {
    "@angular/animations": "~10.0.7",
    "@angular/common": "~10.0.7",
    "@angular/compiler": "~10.0.7",
    "@angular/core": "~10.0.7",
    "@angular/forms": "~10.0.7",
    "@angular/platform-browser": "~10.0.7",
    "@angular/platform-browser-dynamic": "~10.0.7",
    "@angular/router": "~10.0.7",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@ng-select/ng-select": "^4.0.4",
    "@ngxs/form-plugin": "^3.6.2",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/storage-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "bootstrap": "^4.5.0",
    "bootswatch": "^4.5.0",
    "ng-svg-icon-sprite": "^1.8.1",
    "ngx-bootstrap": "^5.6.1",
    "rxjs": "~6.6.2",
    "svg4everybody": "^2.1.9",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.0",
    "@angular-devkit/build-angular": "^0.1000.5",
    "@angular-eslint/builder": "^0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin": "^0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "^0.0.1-alpha.32",
    "@angular-eslint/template-parser": "^0.0.1-alpha.32",
    "@angular/cli": "~10.0.5",
    "@angular/compiler-cli": "~10.0.7",
    "@angular/language-service": "~10.0.7",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@types/jasmine": "~3.5.12",
    "@types/jasminewd2": "~2.0.8",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/svg4everybody": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "htmlhint": "^0.14.1",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jest-allure": "^0.1.3",
    "jest-html-reporters": "^2.0.2",
    "jest-nunit-reporter": "^1.3.1",
    "jest-preset-angular": "^8.2.1",
    "jest-sonar": "^0.2.10",
    "jest-standard-reporter": "^1.0.4",
    "jest-stare": "^2.0.1",
    "jest-trx-results-processor": "^2.0.3",
    "jest-xunit": "^1.0.11",
    "lint-staged": "^10.2.11",
    "ng-mocks": "^10.1.1",
    "prettier": "^2.0.5",
    "standard": "^14.3.4",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "svg2sprite-cli": "^2.0.1",
    "testcafe": "^1.8.8",
    "testcafe-angular-selectors": "^0.4.1",
    "testcafe-reporter-html": "^1.4.6",
    "testcafe-reporter-nunit": "^0.1.2",
    "testcafe-reporter-xunit": "^2.1.0",
    "ts-jest": "^26.1.4",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "~3.9.7",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
