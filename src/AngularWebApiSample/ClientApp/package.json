{
  "name": "client-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ngcc": "ngcc",
    "start": "ng serve",
    "build": "ng build",
    "prod": "ng build --prod",
    "test": "jest --watch",
    "test2": "ng test",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "lint2": "eslint -c .eslintrc.js --ext .ts .",
    "eslint": "eslint -c .eslintrc.js -o ../../../reports/eslint-result.json -f json --ext .ts .",
    "e2e": "testcafe-live chrome e2e/**/*.e2e-spec.ts",
    "e2e:ci": "testcafe chrome e2e/**/*.e2e-spec.ts --app \"yarn start\" --app-init-delay 40000 --proxy-bypass http://localhost:4200/",
    "format": "prettier {src,e2e}/**/*.{ts,md,scss} !src/api/**/*.ts --write",
    "style": "yarn format && yarn lint",
    "verify": "yarn format -l && yarn lint && jest --bail",
    "precommit": "lint-staged",
    "ts-lint": "tslint --project tsconfig.json -c tslint.json -o ../../../reports/tslint-result.json -t json --force src/**/*.ts",
    "ts-lint-console": "tslint --project tsconfig.json -c tslint.json --force src/**/*.ts",
    "1postinstall": "ngcc",
    "stats": "ng build --stats-json",
    "stats-prod": "ng build --prod --stats-json",
    "stats-report": "webpack-bundle-analyzer ../wwwroot/stats.json",
    "type-check": "tsc --noEmit",
    "ts-lint-simple": "tslint --project tsconfig.json --config tslint.json  -o ../../../reports/epcalculator-tslint-result.json -t json --force src/**/*.ts",
    "stylelint": "stylelint src/**/*.scss --syntax scss -f json>../../../reports/stylelint-report.json",
    "stylelint-check": "stylelint-config-prettier-check",
    "stylelint-console": "stylelint src/**/*.scss --syntax scss",
    "htmlhint-console": "htmlhint src/**/*.html --config .htmlhintrc",
    "htmlhint-simple": "htmlhint src/**/*.html --config .htmlhintrc>../../../reports/htmlhint-report.txt",
    "create-icon-sprite": "svg2sprite ./src/assets/svg ./src/assets/sprites/img.svg --stripAttrs id --stripTags title"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,e2e}/**/*.{scss}": [
      "stylelint --fix"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx},!src/app/api/**": [
      "yarn lint --fix"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md,html,json},!src/app/api/**": [
      "prettier --write"
    ]
  },
  "jestNunitReporter": {
    "outputPath": "../../../reports/frontendunittest/",
    "outputFilename": "frontend-test.nunit.xml"
  },
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@ng-select/ng-select": "^5.0.1",
    "@ngxs/form-plugin": "^3.6.2",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/storage-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "bootstrap": "^4.5.2",
    "bootswatch": "^4.5.2",
    "ng-svg-icon-sprite": "^1.9.1",
    "ngx-bootstrap": "^6.1.0",
    "rxjs": "~6.6.2",
    "svg4everybody": "^2.1.9",
    "tslib": "^2.0.1",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.0",
    "@angular-devkit/build-angular": "^0.1001.0",
    "@angular-eslint/builder": "^0.0.1-alpha.33",
    "@angular-eslint/eslint-plugin": "^0.0.1-alpha.33",
    "@angular-eslint/eslint-plugin-template": "^0.0.1-alpha.33",
    "@angular-eslint/template-parser": "^0.0.1-alpha.33",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@angular/language-service": "~10.1.0",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.8",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/svg4everybody": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "codelyzer": "^6.0.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "htmlhint": "^0.14.1",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jest-allure": "^0.1.3",
    "jest-html-reporters": "^2.0.3",
    "jest-junit": "^11.1.0",
    "jest-nunit-reporter": "^1.3.1",
    "jest-preset-angular": "^8.3.1",
    "jest-sonar": "^0.2.10",
    "jest-standard-reporter": "^1.1.1",
    "jest-stare": "^2.2.0",
    "jest-trx-results-processor": "^2.1.0",
    "jest-xunit": "^1.0.11",
    "lint-staged": "^10.3.0",
    "ng-mocks": "^10.1.2",
    "prettier": "^2.1.1",
    "standard": "^14.3.4",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "svg2sprite-cli": "^2.0.1",
    "testcafe": "^1.9.2",
    "testcafe-angular-selectors": "^0.4.1",
    "testcafe-reporter-html": "^1.4.6",
    "testcafe-reporter-nunit": "^0.1.2",
    "testcafe-reporter-xunit": "^2.1.0",
    "ts-jest": "^26.3.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
