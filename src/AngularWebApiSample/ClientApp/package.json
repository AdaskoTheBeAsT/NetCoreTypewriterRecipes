{
  "name": "adaskothebeast",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "nx workspace-lint && ng lint",
    "eslint": "eslint -c .eslintrc.json -o ../../../reports/eslint-result.json -f json --ext .ts",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "precommit": "lint-staged",
    "stats": "ng build --stats-json",
    "stats-prod": "ng build --configuration production --stats-json",
    "stats-report": "webpack-bundle-analyzer ../wwwroot/stats.json",
    "type-check": "tsc --noEmit",
    "stylelint": "stylelint \"{apps,libs}/**/*.scss\" --syntax scss",
    "stylelint-check": "stylelint-config-prettier-check",
    "stylelint-report-json": "stylelint \"{apps,libs}/**/*.scss\" --syntax scss -f json>../../../reports/stylelint-report.json",
    "stylelint-report-html": "stylelint \"{apps,libs}/**/*.scss\" --custom-formatter=node_modules/stylelint-html-formatter --syntax scss>../../../reports/stylelint-report.html",
    "htmlhint": "htmlhint --config .htmlhintrc \"{apps,libs}/**/*.html\"",
    "htmlhint-report-html": "htmlhint --config .htmlhintrc \"{apps,libs}/**/*.html\" --format html&& mv report.html ../../../reports/htmlhint-report.html || move report.html ../../../reports/htmlhint-report.html",
    "htmlhint-report-txt": "htmlhint --config .htmlhintrc \"{apps,libs}/**/*.html\">../../../reports/htmlhint-report.txt",
    "create-icon-sprite": "svg2sprite ./src/assets/svg ./src/assets/sprites/img.svg --stripAttrs id --stripTags title",
    "prepare": "cd .. && cd .. && cd .. && husky install src/AngularWebApiSample/ClientApp/.husky"
  },
  "private": true,
  "lint-staged": {
    "{apps,libs}/**/*.html,!**/api/**": [
      "htmlhint"
    ],
    "{apps,libs}/**/*.{css,scss},!**/api/**": [
      "stylelint --fix"
    ],
    "{apps,libs}/**/*.{js,jsx,ts,tsx},!**/api/**": [
      "eslint --fix"
    ],
    "{apps,libs}/**/*.{js,jsx,ts,tsx,css,scss,md,html,json},!**/api/**": [
      "prettier --write"
    ]
  },
  "jestNunitReporter": {
    "outputPath": "../../../reports/frontendunittest/apps/client-app/",
    "outputFilename": "frontend-test.nunit.xml"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "@adaskothebeast/http-params-processor": "^3.0.0",
    "@angular/animations": "12.2.0",
    "@angular/common": "12.2.0",
    "@angular/compiler": "12.2.0",
    "@angular/core": "12.2.0",
    "@angular/forms": "12.2.0",
    "@angular/localize": "^12.2.0",
    "@angular/platform-browser": "12.2.0",
    "@angular/platform-browser-dynamic": "12.2.0",
    "@angular/router": "12.2.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ng-select/ng-select": "^7.2.0",
    "@ngxs/form-plugin": "^3.7.2",
    "@ngxs/logger-plugin": "^3.7.2",
    "@ngxs/storage-plugin": "^3.7.2",
    "@ngxs/store": "^3.7.2",
    "@nrwl/angular": "12.6.5",
    "bootstrap": "^4.6.0",
    "bootswatch": "^4.6.0",
    "ng-svg-icon-sprite": "^1.10.0",
    "ngx-bootstrap": "^7.0.0",
    "rxjs": "~6.6.7",
    "svg4everybody": "^2.1.9",
    "tslib": "^2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.0",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "12.2.0",
    "@angular/compiler-cli": "12.2.0",
    "@angular/language-service": "12.2.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ngxs/devtools-plugin": "^3.7.2",
    "@nrwl/cli": "12.6.5",
    "@nrwl/eslint-plugin-nx": "12.6.5",
    "@nrwl/jest": "12.6.5",
    "@nrwl/linter": "12.6.5",
    "@nrwl/tao": "12.6.5",
    "@nrwl/workspace": "12.6.5",
    "@types/jest": "26.0.24",
    "@types/node": "^14.17.9",
    "@types/svg4everybody": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "angular-testcafe": "^2.0.6",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "htmlhint": "^0.15.1",
    "husky": "^7.0.1",
    "import-sort-style-module": "^6.0.0",
    "jest": "27.0.6",
    "jest-allure": "^0.1.3",
    "jest-html-reporters": "^2.1.6",
    "jest-junit": "^12.2.0",
    "jest-nunit-reporter": "^1.3.1",
    "jest-preset-angular": "9.0.5",
    "jest-sonar": "^0.2.12",
    "jest-standard-reporter": "^2.0.0",
    "jest-stare": "^2.2.2",
    "jest-trx-results-processor": "^2.2.0",
    "jest-xunit": "^1.0.11",
    "lint-staged": "^11.1.2",
    "ng-mocks": "^12.4.0",
    "prettier": "2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-html-formatter": "^1.0.0",
    "stylelint-scss": "^3.20.1",
    "svg2sprite-cli": "^2.0.1",
    "testcafe": "^1.15.1",
    "testcafe-angular-selectors": "^0.4.1",
    "testcafe-reporter-html": "^1.4.6",
    "testcafe-reporter-nunit": "^0.1.2",
    "testcafe-reporter-xunit": "^2.1.0",
    "ts-jest": "27.0.4",
    "ts-node": "~10.1.0",
    "typescript": "4.3.5",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
