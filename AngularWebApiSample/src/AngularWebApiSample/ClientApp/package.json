{
  "name": "client-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prod": "ng build --prod",
    "test": "jest --watch",
    "test2": "ng test",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "eslint": "eslint -c .eslintrc.js --ext .ts ./",
    "e2e": "testcafe-live chrome e2e/**/*.e2e-spec.ts",
    "e2e:ci": "testcafe chrome e2e/**/*.e2e-spec.ts --app \"yarn start\" --app-init-delay 40000 --proxy-bypass http://localhost:4200/",
    "format": "prettier {src,e2e}/**/*.{ts,md,scss} --write",
    "style": "yarn format && yarn lint",
    "verify": "yarn format -l && yarn lint && jest --bail",
    "precommit": "lint-staged",
    "ts-lint": "tslint --project tsconfig.json -c tslint.json -o ../../../Cake/.artifacts/tslint-result.json -t json --force src/**/*.ts",
    "ts-lint-console": "tslint --project tsconfig.json -c tslint.json --force src/**/*.ts",
    "1postinstall": "ngcc",
    "stats": "ng build --stats-json",
    "stats-prod": "ng build --prod --stats-json",
    "stats-report": "webpack-bundle-analyzer ../wwwroot/stats.json",
    "type-check": "tsc --noEmit",
    "ts-lint-simple": "tslint --project tsconfig.json --config tslint.json  -o ../../../reports/epcalculator-tslint-result.json -t json --force src/**/*.ts",
    "stylelint-simple": "stylelint src/**/*.scss --syntax scss -f json>../../../reports/stylelint-report.json",
    "htmlhint-simple": "htmlhint src/**/*.html --config .htmlhintrc>../../../reports/htmlhint-report.txt",
    "create-icon-sprite": "svg2sprite ./src/assets/svg ./src/assets/sprites/img.svg --stripAttrs id --stripTags title"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx},!src/app/api/**": [
      "yarn lint",
      "git add"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md},!src/app/api/**": [
      "prettier --write",
      "git add"
    ]
  },
  "jestSonar": {
    "reportPath": "../../../reports/frontendunittest/",
    "reportFile": "frontend-test.sonar.xml",
    "indent": 4
  },
  "jestNunitReporter": {
    "outputPath": "../../../reports/frontendunittest/",
    "outputFilename": "frontend-test.nunit.xml"
  },
  "dependencies": {
    "@angular/animations": "~9.0.4",
    "@angular/common": "~9.0.4",
    "@angular/compiler": "~9.0.4",
    "@angular/core": "~9.0.4",
    "@angular/forms": "~9.0.4",
    "@angular/platform-browser": "~9.0.4",
    "@angular/platform-browser-dynamic": "~9.0.4",
    "@angular/router": "~9.0.4",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@ng-bootstrap/ng-bootstrap": "^6.0.0",
    "@ng-select/ng-select": "^3.7.2",
    "@ngxs/form-plugin": "^3.6.2",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/storage-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "bootstrap": "^4.4.1",
    "bootswatch": "^4.4.1",
    "ng-svg-icon-sprite": "^1.7.1",
    "ngx-bootstrap": "^5.3.2",
    "rxjs": "~6.5.4",
    "svg4everybody": "^2.1.9",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.0",
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.4",
    "@angular/language-service": "~9.0.4",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@types/jasmine": "~3.5.7",
    "@types/jasminewd2": "~2.0.8",
    "@types/jest": "^25.1.3",
    "@types/node": "^12.12.29",
    "@types/svg4everybody": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "codelyzer": "^5.2.1",
    "eslint": "^6.8.0",
    "htmlhint": "^0.11.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-html-reporter": "^2.8.0",
    "jest-nunit-reporter": "^1.3.1",
    "jest-preset-angular": "^8.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-standard-reporter": "^1.0.4",
    "jest-xunit": "^1.0.11",
    "lint-staged": "^10.0.8",
    "ng-mocks": "^9.0.0",
    "prettier": "^1.19.1",
    "stylelint": "^13.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.14.2",
    "svg2sprite-cli": "^2.0.1",
    "testcafe": "^1.8.2",
    "testcafe-angular-selectors": "^0.3.2",
    "testcafe-reporter-html": "^1.4.6",
    "testcafe-reporter-nunit": "^0.1.2",
    "testcafe-reporter-xunit": "^2.1.0",
    "ts-node": "~8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "~3.7.5",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "resolutions": {
    "jest": "^25.1.0",
    "tslint": "^6.0.0"
  }
}
