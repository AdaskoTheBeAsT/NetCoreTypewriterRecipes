{
  "name": "client-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prod": "ng build --prod",
    "test": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "testcafe-live chrome e2e/**/*.e2e-spec.ts",
    "e2e:ci": "testcafe all e2e/**/*.e2e-spec.ts --app \"yarn start\" --app-init-delay 5000",
    "format": "prettier {src,e2e}/**/*.{ts,md,scss} --write",
    "style": "yarn format && yarn lint",
    "verify": "yarn format -l && yarn lint && jest --bail",
    "precommit": "lint-staged",
    "ts-lint": "tslint --project tsconfig.json -c tslint.json -o ../../../Cake/.artifacts/tslint-result.json -t json --force src/**/*.ts",
    "ts-lint-console": "tslint --project tsconfig.json -c tslint.json --force src/**/*.ts"
  },
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx},!src/app/api/**": [
      "yarn lint",
      "git add"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md},!src/app/api/**": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.3",
    "@angular/common": "^9.0.3",
    "@angular/compiler": "^9.0.3",
    "@angular/core": "^9.0.3",
    "@angular/forms": "^9.0.3",
    "@angular/platform-browser": "^9.0.3",
    "@angular/platform-browser-dynamic": "^9.0.3",
    "@angular/router": "^9.0.3",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@ngxs/form-plugin": "^3.6.2",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/storage-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "bootstrap": "^4.4.1",
    "bootswatch": "^4.4.1",
    "classlist.js": "^1.1.20150312",
    "ngx-bootstrap": "^5.3.2",
    "rxjs": "^6.5.4",
    "tslib": "^1.11.1",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "^9.0.3",
    "@angular/compiler-cli": "^9.0.3",
    "@angular/language-service": "^9.0.3",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@types/node": "^12.12.28",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "codelyzer": "^5.2.1",
    "husky": "^4.2.3",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.2",
    "lint-staged": "^10.0.8",
    "ng-mocks": "^9.0.0",
    "prettier": "^1.19.1",
    "testcafe": "^1.8.2",
    "testcafe-angular-selectors": "^0.3.2",
    "testcafe-live": "^0.1.4",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "~3.7.5"
  }
}
