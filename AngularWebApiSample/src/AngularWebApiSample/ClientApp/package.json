{
  "name": "client-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prod": "ng build --prod",
    "test": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "testcafe-live chrome e2e/**/*.e2e-spec.ts",
    "e2e:ci": "testcafe all e2e/**/*.e2e-spec.ts --app \"yarn start\" --app-init-delay 5000",
    "format": "prettier {src,e2e}/**/*.{ts,md,scss} --write",
    "style": "yarn format && yarn lint",
    "verify": "yarn format -l && yarn lint && jest --bail",
    "precommit": "lint-staged",
    "ts-lint": "tslint --project tsconfig.json -c tslint.json -o ../../../Cake/.artifacts/tslint-result.json -t json --force src/**/*.ts",
    "ts-lint-console": "tslint --project tsconfig.json -c tslint.json --force src/**/*.ts"
  },
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx},!src/app/api/**": [
      "yarn lint",
      "git add"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md},!src/app/api/**": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.9",
    "@angular/common": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/forms": "^6.1.9",
    "@angular/http": "^6.1.9",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/router": "^6.1.9",
    "@ngxs/devtools-plugin": "^3.2.0",
    "@ngxs/form-plugin": "^3.2.0",
    "@ngxs/logger-plugin": "^3.2.0",
    "@ngxs/storage-plugin": "^3.2.0",
    "@ngxs/store": "^3.2.0",
    "bootstrap": "^4.1.3",
    "bootswatch": "^4.1.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.7",
    "ngx-bootstrap": "^3.0.1",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.3",
    "@angular/cli": "^6.2.3",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/language-service": "^6.1.9",
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@types/node": "~8.9.5",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "codelyzer": "~4.4.4",
    "husky": "^1.0.0",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.1",
    "lint-staged": "^7.3.0",
    "ng-mocks": "^6.2.1",
    "prettier": "^1.14.3",
    "testcafe": "^0.22.0",
    "testcafe-angular-selectors": "^0.3.0",
    "testcafe-live": "^0.1.3",
    "ts-node": "~5.0.1",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "~2.9.2"
  }
}
