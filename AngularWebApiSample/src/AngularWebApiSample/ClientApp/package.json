{
  "name": "client-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prod": "ng build --prod",
    "test": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "testcafe-live chrome e2e/**/*.e2e-spec.ts",
    "e2e:ci": "testcafe all e2e/**/*.e2e-spec.ts --app \"yarn start\" --app-init-delay 5000",
    "format": "prettier {src,e2e}/**/*.{ts,md,scss} --write",
    "style": "yarn format && yarn lint",
    "verify": "yarn format -l && yarn lint && jest --bail",
    "precommit": "lint-staged",
    "ts-lint": "tslint --project tsconfig.json -c tslint.json -o ../../../Cake/.artifacts/tslint-result.json -t json --force src/**/*.ts",
    "ts-lint-console": "tslint --project tsconfig.json -c tslint.json --force src/**/*.ts"
  },
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx},!src/app/api/**": [
      "yarn lint",
      "git add"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md},!src/app/api/**": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ngxs/devtools-plugin": "^3.6.0",
    "@ngxs/form-plugin": "^3.6.0",
    "@ngxs/logger-plugin": "^3.6.0",
    "@ngxs/storage-plugin": "^3.6.0",
    "@ngxs/store": "^3.6.0",
    "bootstrap": "^4.4.1",
    "bootswatch": "^4.4.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.11",
    "ngx-bootstrap": "^5.2.0",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.5.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "^8.3.20",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@types/node": "~12.12.17",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "codelyzer": "~5.2.0",
    "husky": "^3.1.0",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.2",
    "lint-staged": "^9.5.0",
    "ng-mocks": "^8.1.0",
    "prettier": "^1.19.1",
    "testcafe": "^1.7.0",
    "testcafe-angular-selectors": "^0.3.2",
    "testcafe-live": "^0.1.4",
    "ts-node": "~8.5.4",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "~3.5.3"
  }
}
